ia-label=\"Toggle navigation\">\n        <span className=\"navbar-toggler-icon\"></span>\n      </button>\n      <div className=\"collapse navbar-collapse\" id=\"navbar\">\n        <ul className=\"navbar-nav mr-auto\">\n          <li className=\"nav-item\">\n            <Link className=\"nav-link\" to=\"/discover\">Discover</Link>\n          </li>\n        </ul>\n        {\n          useHamburger ?\n            (\n              currentUser ?\n                <ul className=\"navbar-nav\">\n                  <li className=\"nav-item\">\n                    <Link className=\"nav-link\" to=\"/profile\">Profile</Link>\n                  </li>\n                  <li className=\"nav-item\">\n                    <Link className=\"nav-link\" to=\"/edit-profile\">Settings</Link>\n                  </li>\n                  <li className=\"nav-item\">\n                    <div className=\"cursor-pointer nav-link\" onClick={ handleLogout }>Logout</div>\n                  </li>\n                </ul> :\n                <ul className=\"navbar-nav\">\n                  <li className=\"nav-item\">\n                    <Link className=\"nav-link\" to=\"/login\">Login</Link>\n                  </li>\n                  <li className=\"nav-item\">\n                    <Link className=\"nav-link\" to=\"/signup\">Sign Up</Link>\n                  </li>\n                </ul>\n            ) :\n            <ul className=\"navbar-nav ml-auto\">\n              {\n                currentUser ?\n                  <>\n                    <li className=\"dropdown\">\n                      <a className=\"dropdown-toggle nav-link\" href=\"#\" id=\"userDropdown\" role=\"button\" data-toggle=\"dropdown\" aria-haspopup=\"true\" aria-expanded=\"false\">\n                        <img\n                          alt=\"user avatar\"\n                          className=\"align-self-center rounded-circle\"\n                          data-holder-rendered=\"true\"\n                          src={ currentUser.photoURL }\n                          style={{ maxHeight: \"2em\" }}\n                        ></img>\n                        <span className=\"ml-2\">\n                          { currentUser.displayName }\n                        </span>\n                      </a>\n                      <ul className=\"dropdown dropdown-menu dropdown-menu-dark dropdown-menu-right\" aria-labelledby=\"userDropdown\">\n                        <li><Link className=\"dropdown-item py-2\" to=\"/profile\">Profile</Link></li>\n                        <li><Link className=\"dropdown-item py-2\" to=\"/edit-profile\">Settings</Link></li>\n                        <li><hr className=\"dropdown-divider\" /></li>\n                        <li><div className=\"cursor-pointer dropdown-item py-2\" onClick={ handleLogout }>Logout</div></li>\n                      </ul>\n                    </li>\n                  </> :\n                  <>\n                    <li className=\"nav-item\">\n                      <Link className=\"nav-link\" to=\"/login\">Login</Link>\n                    </li>\n                    <li className=\"nav-item\">\n                      <Link className=\"nav-link\" to=\"/signup\">Sign Up</Link>\n                    </li>\n                  </>\n              }\n            </ul>\n        }\n      </div>\n    </nav>\n  )\n}\n\nexport default Navbar\n",["109","110"],"/Users/kimvitug/Projects/hoppr/src/components/Banner.js",[],"/Users/kimvitug/Projects/hoppr/src/components/LocationCard.js",[],"/Users/kimvitug/Projects/hoppr/src/components/Footer.js",[],"/Users/kimvitug/Projects/hoppr/src/components/Login.js",[],["111","112"],"/Users/kimvitug/Projects/hoppr/src/components/SignUp.js",[],"/Users/kimvitug/Projects/hoppr/src/components/UserProfile.js",[],"/Users/kimvitug/Projects/hoppr/src/firebase.js",[],["113","114"],"/Users/kimvitug/Projects/hoppr/src/contexts/AuthContext.js",[],"/Users/kimvitug/Projects/hoppr/src/components/PrivateRoute.js",[],"/Users/kimvitug/Projects/hoppr/sr